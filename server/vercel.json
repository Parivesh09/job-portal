{
    "version": 2,
    "functions": {
        "controllers/*.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}